- if current_user
  %p= "Your chat token is:"
  %h1= "#{current_user.chat_token}"
  %p= link_to "Generate new token", create_chat_token_path, method: 'post'
  %p= link_to "Click here to logout", destroy_user_session_path, method: 'delete'
- else
  %p= "You need to be logged in to access this page"
  %p= link_to "Click here to login", new_user_session_path
